page{background:#f3f5f7;font-size: 0.8rem;}
.ov{overflow: hidden;}
.fl{float: left;}
.fr{float: right;}
.cf2{color: #f23030;}
.order_list_top{background: #fff;padding:0.6rem 2.5rem 0.9rem 0.7rem;position: relative;border-top:1px solid #ddd;margin-bottom:0.5rem;}
.order_list_top .imt image{display: block;width: 100%;height:2px;position: absolute;left: 0;bottom: 0;}
.order_list_top .imb image{display: block;position:absolute;right:0.5rem;top:50%;margin-top:-0.45rem;width:0.5rem;height:0.89rem;}
.order_list_top .mt{padding-left:1.5rem;position: relative;}
.order_list_top .top{font-weight:bold;font-size:0.75rem;color:#222;margin-bottom:0.3rem;}
.order_list_top .top_name{padding-right:1.5rem;}
.order_list_top .address{color: #666;}
.order_list_top .address image{display: block;width:0.9rem;height:1.125rem;position: absolute;left: 0;top:-0.1rem;}
.order_list_lt{background: #fff;border-top:1px solid #edeef0;border-bottom:1px solid #edeef0;}
.order_list_lt .title{height:2.3rem;line-height:2.3rem;padding:0 0.8rem;}
.order_list_lt .title image{display:inline-block;width: 1.0rem;height: 1.0rem;vertical-align: middle;margin:-0.2rem 0.5rem 0 0;}
.order_list_lt .list{background: #f8f8f8;padding:0 0.6rem;}
.order_list_lt .list_lt{padding:0.5rem 0;border-bottom:1px solid #f0f1f3;overflow: hidden;position: relative;}
.order_list_lt .llt_fl{float: left;width: 20%;}
.order_list_lt .llt_fl image{width: 100%;}
.order_list_lt .llt_fr{float: right;width:77%;line-height:150%;}
.order_list_lt .llr_dom{position: absolute;bottom:0.5rem;width:77%;font-weight:bold;}
.order_list_lt .llr_dom .dom_vule{float: left;color: #f23030;font-size:0.7rem;}
.order_list_lt .llr_dom .dom_num{float: right;color: #333;font-weight: normal;}
.order_list_lt .dom .dom_all{height:2.2rem;line-height: 2.2rem;text-align: right;font-size: 0.75rem;padding-right:0.5rem;border-bottom:1px solid #f0f1f3;}
.order_list_lt .dom .dom_all .cf2{font-size: 0.7rem;padding:0 0.4rem;}
.order_list_lt .dom .dom_caozuo{text-align: right;height: 2.2rem;border-bottom:1px solid #f0f1f3;}
.order_list_lt .dom .dom_caozuo button{display: inline-block;width:4.0rem;height:1.6rem;line-height: 1.65rem;padding: 0;font-size:0.75rem;border-radius: 3px;background: #fff;margin:0.3rem 0.5rem 0 0;}
.order_list_lt .dom .dom_caozuo button.active{border:1px solid #f23030;color: #f23030;}
.order_list_lt .none{background: #f0f2f5;padding-bottom:0.5rem;border-bottom:1px solid #f0f1f3;}
.order_list_lt .none image{width: 100%;}
.order_list_search{margin:0.5rem 0.7rem;}
.order_list_search .search_form{background: #fff;display: block;width: 100%;height: 100%;border-radius:3px;position: relative;overflow: hidden;}
.order_list_search input{width:100%;height:1.8rem;border-radius:3px;text-align: center;}
.order_list_search button{width:1.8rem;height:1.8rem;position: absolute;right:0;top: 0;z-index: 9;padding:0;background: #fff;}
.order_list_search button image{display: block;width:1.0rem;height:1.0rem;margin:0.4rem auto 0;}
.order_list_menu{background: #fff;overflow: hidden;margin-bottom:0.5rem;}
.order_list_menu .lt{float: left;width:16%;text-align: center;height:2.2rem;line-height: 2.2rem;margin:0 2%;}
.order_list_menu .lt.active{border-bottom:2px solid #f23030;color: #f23030;}
.order_list_dom{height: 2.3rem;line-height: 2.3rem;background: #f8f8f8;border-top:1px solid #f0f1f3;padding:0 0.5rem;color: #999;}
.order_list_dom .btn_delete{padding:0;padding:0 0.5rem;font-size:0.7rem;height: 1.7rem;line-height: 1.7rem;margin-top:0.25rem;border:1px solid #ccc;background: #fff;}

/*弹窗样式  */
.tanc_beij{background: rgba(0,0,0,0.8);position: fixed;left: 0;top: 0;width: 100%;height: 100%;z-index: 9;}
.tanc_text{width: 80%;background: #fff;position: fixed;left: 10%;top: 15%;border-radius:0.3rem;overflow: hidden;z-index:10;}
.tanc_title{height: 2.7rem;line-height: 2.7rem;text-align: center;}
.tanc_cancel .lt{height: 3.0rem;line-height: 3.0rem;padding:0 1.0rem;position:relative;border-bottom:1px solid #dedede;}
.tanc_btn{display: flex;}
.tanc_btn .btn{width: 50%;border-radius: 0;font-size:0.8rem;padding: 0;height: 3.0rem;line-height: 3.0rem;}











