page{height:100%;}
 #list{overflow: hidden;font-size:0.9rem;height:100%;background:#f3f5f7;}
.list_fl{width: 30%;float: left;height: 100%;text-align: center;background: #fff;}
.list_fr{width: 70%;float: right;height: 100%;margin-left: -2px;} 
.fl_list .list_lt{height: 2.5rem;line-height:2.5rem;font-size: 0.75rem;}
.fr_content{padding:0.8rem 0.5rem 0;margin-left: 2px;font-size: 0.75rem;}
.fr_imt image{width: 100%;height: 5.0rem;}
.fr_title{text-align: center;color:#333;padding: 0.4rem 0;}
.fr_list{overflow: hidden;}
.fr_list .list_lt{width: 33.3%;float: left;text-align: center;font-size:0.75rem}
.fr_list .lt_imt image{width:3.8rem;height:3.8rem;}
.fr_list .lt_name{padding:0.2rem 0 0.8rem;white-space: nowrap;text-overflow: ellipsis;overflow: hidden;}
.fl_list,.fl_list scroll-view{height: 100%;}
.list_fr,.fr_content,.fr_content scroll-view{height: 100%}
















